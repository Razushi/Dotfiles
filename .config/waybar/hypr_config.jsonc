{
  "spacing": 4,
  "position": "top",
  "layer": "top",

  "margin-top": 0,
  "margin-right": 0,
  "margin-bottom": 0,
  "margin-left": 0,

  "modules-left": [
    "custom/app-runner",
    "hyprland/workspaces",
    "mpris"
  ],
  "modules-center": [
    "hyprland/window"
  ],
  "modules-right": [
    "tray",
    "pulseaudio",
    "battery",
    "network",
    "clock",
    "custom/notification",
    "idle_inhibitor"
  ],

  "hyprland/workspaces": {
    "persistent-workspaces": {
      "DP-2": [1, 2, 3, 4, 5],
      "HDMI-A-2": [6, 7, 8, 9, 10],
      "eDP-1": [1, 2, 3, 4, 5]
    },
    "format": "{icon}",
    "format-icons": {
      "1": "ä¸€",
      "2": "äºŒ",
      "3": "ä¸‰",
      "4": "å››",
