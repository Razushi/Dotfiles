    "tooltip-format": "{essid} via {gwaddr}",
    "format-linked": "{ifname} (No IP)",
    "format-disconnected": "Disconnected âš "
  },

  "battery": {
    "interval": 15,
    "states": {
      "warning": 40,
      "critical": 15
    },
    "format": "{icon} {capacity}%",
    "format-charging": "ïƒ§ {capacity}% ({timeToFull})",
    "format-plugged": "ï‡¦ {capacity}%",
