{
  "spacing": 4,
  "position": "top",
  "layer": "top",

  "margin-top": 0,
  "margin-right": 0,
  "margin-bottom": 0,
  "margin-left": 0,

  "modules-left": [
    "custom/app-runner",
    "hyprland/workspaces",
    "mpris"
  ],
  "modules-center": [
    "hyprland/window"
  ],
  "modules-right": [
    "tray",
    "pulseaudio",
    "battery",
    "network",
    "clock",
    "custom/notification",
    "idle_inhibitor"
  ],

  "hyprland/workspaces": {
    "persistent-workspaces": {
      "DP-2": [1, 2, 3, 4, 5],
      "HDMI-A-2": [6, 7, 8, 9, 10],
      "eDP-1": [1, 2, 3, 4, 5]
    },
    "format": "{icon}",
    "format-icons": {
      "1": "‰∏Ä",
      "2": "‰∫å",
      "3": "‰∏â",
      "4": "Âõõ",
      "5": "‰∫î",
      "6": "ÂÖ≠",
      "7": "‰∏É",
      "8": "ÂÖ´",
      "9": "‰πù",
      "10": "ÂçÅ"
    }
  },

  "hyprland/window": {
    "max-length": 50
  },

  "keyboard-state": {
    "numlock": true,
    "capslock": true,
    "format": "{name} {icon}",
    "format-icons": {
      "locked": "ÔÄ£",
      "unlocked": "ÔÇú"
    }
  },

  "mpris": {
    "format": "{player_icon} {dynamic}",
    "format-paused": "{status_icon} <i>{dynamic}</i>",
    "player-icons": {
      "default": "‚è∏ ",
      "mpv": "üéµ"
    },
    "status-icons": {
      "paused": "‚ñ∂ "
    },
    "dynamic-separator": " ",
    "interval": 2,
    "dynamic-order": ["title"],
    "max-length": 30
  },

  "idle_inhibitor": {
    "format": "{icon}",
    "format-icons": {
      "activated": "ÔÅÆ",
      "deactivated": "ÔÅ∞"
    }
  },

  "tray": {
    "spacing": 24
  },

  "clock": {
    "tooltip-format": "<big>{:%Y %B}</big>\n<tt><small>{calendar}</small></tt>",
    "format": "{:%H:%M ÔÄó  }"
  },

  "network": {
    "format-wifi": "({signalStrength}%) Ôá´",
    "format-ethernet": "{ipaddr}/{cidr}",
    "tooltip-format": "{essid} via {gwaddr}",
    "format-linked": "{ifname} (No IP)",
    "format-disconnected": "Disconnected ‚ö†"
  },

  "battery": {
    "states": {
      "warning": 50,
      "critical": 20
      },
    "format": "{icon}",
    "format-charging": "",
    "format-plugged": "",
    // "format-good": "", // An empty format will hide the module
    // "format-full": "",
    "format-icons": ["Ôïπ", "Ôïª", "ÔïΩ", "Ôïø", "Ôï∏"]
  },

  "battery#bat2": {
    "bat": "BAT2"
  },

  "pulseaudio": {
    "format": "{icon}{volume}% {format_source}",
    "format-bluetooth": "{icon}Ôäî {volume}% {format_source}",
    "format-bluetooth-muted": "Ôë¶  Ôäî {volume}% {format_source}",
    "format-muted": "Ôë¶  {format_source}",
    "format-source": "ÔÑ∞ ",
    "format-source-muted": "ÔÑ± ",
    "format-icons": {
      /* "headphone": "ÔÄ•   ", */
      "default": ["ÔÄ¶  ", "ÔÄß  ", "ÔÄ®  "]
    },
    "tooltip-format": "{desc}, {volume}%",
    "on-click-right": "pavucontrol",
    "on-click": "wpctl set-mute @DEFAULT_SOURCE@ toggle"
  },

  "custom/power": {
    "format": "‚èª ",
    "tooltip": false,
    "menu": "on-click",
    "menu-file": "$HOME/.config/waybar/power_menu.xml",
    "menu-actions": {
      "shutdown": "shutdown",
      "reboot": "reboot",
      "suspend": "systemctl suspend",
      "hibernate": "systemctl hibernate"
    }
  },

  "custom/app-runner": {
    "format": "Ôåì ",
    "on-click": "fuzzel",
    "tooltip": false
  },

  "custom/notification": {
    "tooltip": false,
    "format": "{icon}",
    "format-icons": {
      "notification": "ÔÇ¢<span foreground='red'><sup>ÔëÑ</sup></span>",
      "none": "ÔÇ¢",
      "dnd-notification": "Ôá∑<span foreground='red'><sup>ÔëÑ</sup></span>",
      "dnd-none": "Ôá∑",
      "inhibited-notification": "ÔÇ¢<span foreground='red'><sup>ÔëÑ</sup></span>",
      "inhibited-none": "ÔÇ¢",
      "dnd-inhibited-notification": "Ôá∑<span foreground='red'><sup>ÔëÑ</sup></span>",
      "dnd-inhibited-none": "Ôá∑"
    },
    "return-type": "json",
    "exec-if": "which swaync-client",
    "exec": "swaync-client -swb",
    "on-click": "swaync-client -t -sw",
    "on-click-right": "swaync-client -d -sw",
    "escape": true
  }
}
